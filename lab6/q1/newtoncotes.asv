%newton-cotes formulas for n=4,6,9
% formula source: www.wolframalpha.com
%--------------------------------------------------------------------------
f=inline('x^4');% defining the required function
a=input('lower limit');
b=input('upper limit');
%--------------------------------------------------------------------------
% for n=4 (simpsons 3/8 rule)
% The 4-point closed rule is Simpson's 3/8 rule,
%int_f(x)dx=3/8h(f_1+3f_2+3f_3+f_4)-3/(80)h^5f^((4))(xi) 
%--------------------------------------------------------------------------
h=(b-a)/4;
t=[a:h:b];
int1=(3/8)*h*(f(t(1))+3*f(t(2))+3*f(t(3))+f(t(4)));
%--------------------------------------------------------------------------
% for n=6 
% The 6-point closed rule
%int_f(x)dx=5/(288)h(19f_1+75f_2+50f_3+50f_4+75f_5+19f_6)-(275)/(12096)h^7f^((6))(xi),  
%--------------------------------------------------------------------------
h=(b-a)/6;
t=[a:h:b];
int2=(5/288)*h*(19*f(t(1))+75*f(t(2))+50*f(t(3))+50*f(t(4))+75*f(t(5))+19*f(t(6)));
