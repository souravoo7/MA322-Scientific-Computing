%--------------------------------------------------------------------------
%   Euler's Explicit Method 
%--------------------------------------------------------------------------
k=explicit(0,1,40);
h=1/40;
l=[0:h:1-h];
odeset('InitialStep',0.1);
%--------------------------------------------------------------------------
%   Euler's Implicit Method
%--------------------------------------------------------------------------
h1=1/100;
l1=[0:h1:1-h1];
k1=implicit(0,1,100,0.1)
%--------------------------------------------------------------------------
%   Using ode23(inbuilt matlab function using Runge Kutta 2/3 order) 
%--------------------------------------------------------------------------
f=inline('sin(t)-y');%function definition
[T,Y]=ode23(f,[0,1],1);
%--------------------------------------------------------------------------
%   Using ode45(inbuilt matlab function using Runge Kutta 4/5 order)
%--------------------------------------------------------------------------
[T1,Y1]=ode45(f,[0,1],1);
%--------------------------------------------------------------------------
%   Using ode23t(inbuilt matlab function using trapezoidal rule)
%--------------------------------------------------------------------------
[T2,Y2]=ode23t(f,[0,1],1);
plot(l,k,T,Y,T1,Y1,T2,Y2);

